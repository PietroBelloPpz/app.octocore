<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nannyapp.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NannyApp</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="info@titania.io" href="http://www.titania.io">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="http://nannyapp.cloud" subdomains="true" />
    <access origin="http://app.nannyapp.cloud*" />
